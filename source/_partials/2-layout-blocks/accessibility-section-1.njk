<section class="[ mambo-section ][ flow ][ padding-all-20 padding-top-80
	padding-bottom-80 medium:padding-all-64 large:padding-all-120 ][ {{ sectionOne.sectionOneColorCombo | safe }} ]" data-color="{{ sectionOne.sectionOneColorCombo | safe }}" id="{{ sectionOne.sectionOneHeader | slug }}">
	<div class="[ container-content ][ flex-flow flex-flow-column flex-flow-start ]">
		<div class="[ text ][ flex-flow flex-flow-column flex-flow-start ]">
			<h2>{{ sectionOne.sectionOneHeader | md | safe }}</h2>
			{% include '1-mambo-blocks/divider.njk' %}
			<div class="wysiwyg flow-3">{{ sectionOne.sectionOneBody | md | safe }}</div>
		</div>
	</div>
</section>
