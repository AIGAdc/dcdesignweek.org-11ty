<section class="[ small-sponsors ][ flow ][ padding-all-16 ]" id="sponsors">
	{% for content in sponsors.global %}
	<h2>
		{{ content.header }}
	</h2>
	<p>
		{{ content.body | safe }}
	</p>
	{% endfor %}
	{% include 'block/sponsors/list.njk' %}
	{% for content in sponsors.global %}
	<a class="[ mambo-button ] [ {{ content.buttonBGColor }} {{ content.buttonTextColor }} ]"
		href="{{ content.buttonURL }}">
		{{ content.buttonLabel }}
	</a>
	{% endfor %}
</section>
